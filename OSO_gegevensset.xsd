              <xs:element name="tussenvoegsel" type="nonEmptyStringType" minOccurs="0"/>
              <xs:element name="achternaam" type="nonEmptyStringType"/>
              <xs:element name="geslacht" type="geslachtType"/>
              <xs:element name="geboortedatum" type="xs:date"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="terugkoppelinformatie" type="terugkoppelinformatieType"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
