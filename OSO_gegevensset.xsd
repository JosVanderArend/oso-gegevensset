      <xs:element name="afnamedatum" type="xs:date" minOccurs="0"/>
      <xs:element name="jaargroep" type="jaargroepType" minOccurs="0"/>
      <xs:element name="periode" type="nonEmptyStringType" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="cijferlijstTypelijstType">
