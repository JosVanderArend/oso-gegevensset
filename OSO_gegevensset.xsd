    <xs:sequence>
      <xs:element name="tiperschooljaar" minOccurs="0" maxOccurs="unbounded">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="schooljaar" type="schooljaarType"/>
            <xs:element name="begindatum" type="xs:date"/>
            <xs:element name="einddatum" type="xs:date"/>
            <xs:element name="groepsnaam" type="nonEmptyStringType" minOccurs="0"/>
            <xs:element name="jaargroep" type="jaargroepType"/>
            <xs:element name="iltcode" type="nonEmptyStringType"/>
            <xs:element name="iltomschrijving" type="nonEmptyStringType"/>
