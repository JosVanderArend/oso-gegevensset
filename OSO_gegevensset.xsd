      <xs:pattern value="[0-9][0-9][0-9][0-9]-[0-9][0-9][0-9][0-9]"/>
    </xs:restriction>
  </xs:simpleType>
  <!-- 20140509 enumeration in restriction verwijderd; validatie van codelijst 13 in XSLT -->
  <!-- 20131128 VIJFTIEN waarden toegevoegd -->
  <xs:simpleType name="jaargroepType" final="restriction">
    <xs:restriction base="xs:string">
      <xs:annotation>
        <xs:documentation>Toegestane waarden in codelijst 13 in bestand OSO codelijsten.</xs:documentation>
      </xs:annotation>
    </xs:restriction>
  </xs:simpleType>
  <!-- 20160120 type "elementcodeType" toegevoegd voor enkele velden -->
  <xs:simpleType name="elementcodeType" final="restriction">
    <xs:restriction base="xs:string">
      <xs:pattern value="[0-9][0-9][0-9][0-9]"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="toetsscoreType">
    <xs:sequence>
      <xs:element name="aantalopgaven" type="xs:int" minOccurs="0"/>
      <xs:element name="aantalgoed" type="xs:int" minOccurs="0"/>
      <xs:element name="aantalfout" type="xs:int" minOccurs="0"/>
      <xs:element name="aantalgelezen" type="xs:int" minOccurs="0"/>
